<% title "Emp Types" %>
 <%= form_for @search, :html => { :id => "search-form" }   do |search| %>
    <div id="div-search-form">
        <table>
            <tr><h3>Searching Criteria</h3></tr>
            <tr>
                <td><%= search.text_field :emptype_name_contains,:size => 15, :title => "Emp Type" %></td>
                <td><%=search.submit "Search", :name => nil%></td>
                <td><%= reset_form%></td>
            </tr>
        </table>
    </div>

    <div id='emp_types'>
        <%= render 'emp_types'%>
    </div>
<%end%>
